{
  "name": "fractal",
  "version": "1.0.0",
  "description": "A template for a p5.js sketch using Typescript",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "release": "parcel build index.html --public-url ./",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@types/p5": "0.9.0",
    "bulma": "^0.9.2",
    "bulma-slider": "^2.0.4",
    "p5": "0.9.0"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "parcel-bundler": "^1.6.1",
    "sass": "^1.32.8",
    "typescript": "^4.1.5"
  },
  "keywords": [
    "p5js",
    "typescript"
  ]
}